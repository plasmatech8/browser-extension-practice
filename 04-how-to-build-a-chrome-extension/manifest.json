{
    "name": "obj ext",
    "description": "my ext",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
        "128": "./obj-icon-128x128.png"
    },
    "background": {
        "scripts": ["./background.js"]
    },
    "options_ui": {
        "page": "./options.html",
        "open_in_tab": true
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "https://www.google.com/*",
        "https://github.com/*",
        "storage"
    ]
}